<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.holifridge.fridge2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_main"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_main_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name="com.holifridge.fridge2.Activity.AddActivity"/>
        <activity android:name="com.holifridge.fridge2.Activity.CameraActivity" />
        <activity android:name="com.holifridge.fridge2.Activity.SilonActivity" />
        <activity android:name="com.holifridge.fridge2.Activity.DongActivity" />
        <activity android:name="com.holifridge.fridge2.Activity.JangActivity" />
        <activity android:name="com.holifridge.fridge2.Activity.MainActivity" />
        <activity android:name="com.holifridge.fridge2.Activity.PasswordActivity" />
        <activity android:name="com.holifridge.fridge2.Activity.SignUpActivity" />
        <activity android:name="com.holifridge.fridge2.Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>